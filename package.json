{
  "name": "wiki-editor",
  "keywords": [
    "slate",
    "react",
    "wysiwyg",
    "wiki"
  ],
  "version": "0.0.4",
  "description": "React/Slate WYSIWYG Editor",
  "main": "lib/wiki-editor.min.js",
  "repository": "git@github.com:TroyAlford/wiki-editor.git",
  "author": "Troy Alford <troy@troyalford.com>",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.1",
    "immutable": "^3.8.2",
    "prettify-html": "TroyAlford/prettify-html",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "slate": "^0.30.6",
    "slate-auto-replace": "^0.9.0",
    "slate-html-serializer": "^0.4.6",
    "slate-react": "^0.10.11"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-jest": "^21.2.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.0.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "jshint": "^2.9.5",
    "node-sass": "^4.5.2",
    "node-sass-glob-importer": "^5.0.0-alpha.16",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.2.2",
    "publish": "^0.6.0",
    "rollup": "^0.51.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "source-map-explorer": "^1.5.0",
    "webpack": "^3.0.0"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "build-css": "node-sass source/wiki-editor.scss --output-style compressed --source-map true --source-map-embed --importer node_modules/node-sass-glob-importer/dist/cli.js -o lib",
    "lint": "eslint --fix --ignore-pattern /node_modules/,/lib/ ./source/",
    "prebuild": "mkdir -p ./lib ./examples/lib && rm -rf ./lib/* ./examples/lib/*",
    "preversion": "yarn test",
    "publish-to-npm": "publish",
    "sourcemap": "source-map-explorer ./lib/wiki-editor.js",
    "test": "jest",
    "version": "yarn build && git add -A ./lib",
    "watch": "yarn prebuild && (yarn-watch-css & SOURCEMAP=true yarn webpack --watch)",
    "watch-css": "nodemon --watch ./source -e scss --exec 'yarn build-css'"
  },
  "pre-commit": [
    "test"
  ]
}
